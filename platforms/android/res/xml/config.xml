<?xml version='1.0' encoding='utf-8'?>
<widget id="com.oztemur.crowd" version="0.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="DialogGPS">
        <param name="android-package" value="cl.rmd.cordova.dialoggps.DialogGPS" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Globalization">
        <param name="android-package" value="org.apache.cordova.globalization.Globalization" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <feature name="Diagnostic">
        <param name="android-package" value="cordova.plugins.Diagnostic" />
    </feature>
    <allow-intent href="market:*" />
    <name>Crowd</name>
    <description>
    Crowd is an application that provides how places are crowded.
  </description>
    <author email="support@crowd.com" href="http://crowd.com">
    Mahmut Oztemur
  </author>
    <content src="index.html" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-installLocation" value="auto" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <icon density="ldpi" platform="android" src="www/res/icon/android/icon-36-ldpi.png" />
    <icon density="mdpi" platform="android" src="www/res/icon/android/icon-48-mdpi.png" />
    <icon density="hdpi" platform="android" src="www/res/icon/android/icon-72-hdpi.png" />
    <icon density="xhdpi" platform="android" src="www/res/icon/android/icon-96-xhdpi.png" />
    <icon density="xxhdpi" platform="android" src="www/res/icon/android/icon-144-xxhdpi.png" />
    <icon density="xxxhdpi" platform="android" src="www/res/icon/android/icon-192-xxxhdpi.png" />
    <icon platform="blackberry" src="www/res/icon/blackberry/icon-80.png" />
    <icon platform="blackberry" src="www/res/icon/blackberry/icon-80.png" state="hover" />
    <icon height="57" platform="ios" src="www/res/icon/ios/icon-57.png" width="57" />
    <icon height="72" platform="ios" src="www/res/icon/ios/icon-72.png" width="72" />
    <icon height="114" platform="ios" src="www/res/icon/ios/icon-57-2x.png" width="114" />
    <icon height="144" platform="ios" src="www/res/icon/ios/icon-72-2x.png" width="144" />
    <icon platform="webos" src="www/res/icon/webos/icon-64.png" />
    <icon platform="winphone" src="www/res/icon/windows-phone/icon-48.png" />
    <icon platform="winphone" role="background" src="www/res/icon/windows-phone/icon-173-tile.png" />
    <splash density="port-ldpi" platform="android" src="www/res/screen/android/screen-ldpi-portrait.png" />
    <splash density="port-mdpi" platform="android" src="www/res/screen/android/screen-mdpi-portrait.png" />
    <splash density="port-hdpi" platform="android" src="www/res/screen/android/screen-hdpi-portrait.png" />
    <splash density="port-xhdpi" platform="android" src="www/res/screen/android/screen-xhdpi-portrait.png" />
    <splash height="480" platform="ios" src="www/res/screen/ios/screen-iphone-portrait.png" width="320" />
    <splash height="960" platform="ios" src="www/res/screen/ios/screen-iphone-portrait-2x.png" width="640" />
    <splash height="1136" platform="ios" src="www/res/screen/ios/screen-iphone-portrait-568h-2x.png" width="640" />
    <splash height="1024" platform="ios" src="www/res/screen/ios/screen-ipad-portrait.png" width="768" />
    <splash height="768" platform="ios" src="www/res/screen/ios/screen-ipad-landscape.png" width="1024" />
    <access origin="*" />
    <allow-navigation href="http://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
